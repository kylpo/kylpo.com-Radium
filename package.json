{
  "name": "kylpo.com",
  "private": true,
  "scripts": {
    "lint": "eslint site lib",
    "clean": "rimraf public",
    "prebuild": "npm run clean",
    "build": "BUILD_SITE=true webpack",
    "build:dev": "DEV=true npm run build -- -d --progress --colors",
    "build:prod": "PROD=true npm run build -- -p --progress --colors",
    "server:dev": "DEV=true webpack-dev-server -d --inline --hot --progress --history-api-fallback",
    "server:prod": "serve public/ -p 8080",
    "start": "npm run build:dev && npm run server:dev",
    "prod": "npm run build:prod && npm run server:prod",
    "deploy": "npm run build:prod && cp CNAME public/ && gh-pages -d public"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^6.6.0",
    "eslint-config-kylpo": "^1.0.0",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "npm-install-webpack-plugin": "^4.0.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "serve": "^1.4.0",
    "static-site-generator-webpack-plugin": "^2.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "kylpo-BackgroundImage": "^0.1.5",
    "kylpo-Block": "^0.1.5",
    "kylpo-Button": "^0.1.5",
    "kylpo-Col": "^0.1.5",
    "kylpo-Flex": "^0.1.5",
    "kylpo-Inline": "^0.1.5",
    "kylpo-InlineBlock": "^0.1.5",
    "kylpo-InlineCol": "^0.1.5",
    "kylpo-InlineFlex": "^0.1.5",
    "kylpo-InlineRow": "^0.1.5",
    "kylpo-Row": "^0.1.5",
    "kylpo-Text": "^0.1.5",
    "radium": "^0.18.1",
    "radium-normalize": "^2.0.3",
    "react": "^15.1.0",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.1.0",
    "react-html-document": "^2.2.0",
    "react-router": "^2.4.1",
    "react-router-scroll": "^0.2.0"
  }
}
